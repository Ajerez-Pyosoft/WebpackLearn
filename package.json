{
  "name": "webpack4",
  "version": "1.0.0",
  "description": "webpack 4 class",
  "main": "index.js",
  "scripts": {
    "build": "webpack --entry ./index.js --output ./bundle.js --mode development",
    "build:local": "webpack --mode production",
    "build:external": "webpack --config ./external/WPextra.config.js",
    "build:multi": "webpack --config ./multipleEntryPoints/WPextra.config.js",
    "build:css": "webpack --config ./css-style-loaders/WPextra.config.js",
    "build:plugins": "webpack --config ./plugins/WPextra.config.js",
    "build:dev": "webpack-dev-server --config ./servidorDesarrollo/WPextra.config.js",
    "build:babel": "webpack-dev-server --config ./babel/WPextra.config.js",
    "build:react": "webpack-dev-server --config ./react/WPextra.config.js",
    "build:url-loader": "webpack-dev-server --config ./url-loader/WPextra.config.js",
    "build:preprocesadores": "webpack-dev-server --config ./preprocesadores/WPextra.config.js",
    "build:prevent-dup": "webpack --config ./prevent-duplication/WPextra.config.js",
    "build:dll": "webpack --config ./dll/webpack.dll.config.js",
    "build:prevent-dup-fast": "webpack --config ./dll/WPextra.config.js",
    "build:dynamic": "webpack --config ./dynamic-Imports/WPextra.config.js",
    "build:dynamic:dll": "webpack --config ./dynamic-Imports/WPextra.config.js"
  },
  "keywords": [
    "webpack4"
  ],
  "author": "Andres Jerez",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.10.2",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "sass-loader": "^7.3.1",
    "style-loader": "^1.0.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "url-loader": "^2.1.0",
    "webpack": "4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0"
  }
}
